	
	class Nquestion1_6
{
	public static void main(String args[])
	{
		
	for(int i =1;i<=5;i++)
	{                                                                          
	//inner loop --> space print
	for(int j =1;j<=5-i;j++)
	{
	System.out.print(" ");
	}
	//inner loop --> star print
	for(int j=1;j<=i;j++)
	{
	System.out.print("*");
	}
	System.out.println();
  }
 }
}